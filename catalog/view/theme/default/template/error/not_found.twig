{{ header }}
<div class="margin-top"></div>

<div id="error-not-found" class="container-custom">
  <ul class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
    {% endfor %}
  </ul>

  <div class="row vh80 not-found">{{ column_left }}
    {% if column_left and column_right %}
    {% set class = 'col-sm-6' %}
    {% elseif column_left or column_right %}
    {% set class = 'col-sm-9' %}
    {% else %}
    {% set class = 'col-sm-12' %}
    {% endif %}
    <div id="content" class="{{ class }}">{{ content_top }}
      <h1 class="not-found__title">{{ heading_title }}</h1>
      <p class="not-found__text">{{ text_error }}</p>
      <div class="buttons clearfix">
        <div class="not-found__wrapper"><a href="{{ continue }}" class="not-found__btn">{{ button_continue }}</a></div>
      </div>
      {{ content_bottom }}</div>
    {{ column_right }}</div>
</div>
{{ footer }}
